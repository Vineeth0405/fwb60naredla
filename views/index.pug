extends layout

block content
  h1= title
  p Pug templates are used to dynamically generate web pages using #{title}
  button(type="button", class="btn btn-primary" onclick="update()") Press Me 
  div(id='target') Display 
  script.
    let client_count = 0;
    function update(){
      client_count++
      document.getElementById("target").innerHTML = `Value now ${client_count}`
    }
  a(href="vehicles") Go to Vehicles Page
